<% include("partials/header") %>
    <div class="container">
        <header class="jumbotron">
            <h1>This is the campgrounds page!</h1>
        </header>

        <a href="/campgrounds/new">Add New campground</a>

        <%campgrounds.forEach(campground => { %>
            <div>
                <h4>
                    <%= campground.name %>
                </h4>
                <img src="<%= campground.image %>">
            </div>

            <% }); %>
    </div>
    <% include("partials/footer") %>